    "node": ">=4.0.0"
  },
  "scripts": {
    "test-selenium": "./tools/execute-selenium-tests",
    "test-integration": "npm run clean_db && ./node_modules/.bin/mocha test/integration/test/*.js && ./node_modules/.bin/mocha test/integration/test/no_travis/*.js",
